let x= 3.4;
let y= 2;
let z;

z= Math.round(x);
z=Math.floor(x);
z=Math.ceil(x);
z=Math.trunc(x);
z=Math.pow(x);
z=Math.sqrt(x);
z=Math.log(x);
z=Math.sin(x);
z=Math.cos(x);
z=Math.tan(x);
z=Math.abs(x);
z=Math.sign(x);
let max = Math.max(x, y, z);
let min = Math.min(x, y, z);
console.log(min);
